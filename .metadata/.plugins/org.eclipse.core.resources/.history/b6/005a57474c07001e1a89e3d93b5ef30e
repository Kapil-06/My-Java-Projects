package com.kapil.beans;

public class Account {

	private int accno;
	private String accnm, acctype;
	private double balance;
	
	public Account() {
		accno=0;
		accnm="Not Found";
		acctype="Not Found";
		balance=0.0;
	}

	public String getAccnm() {
		return accnm;
	}

	public String getAcctype() {
		return acctype;
	}

	public double getBalance() {
		return balance;
	}

	public void setAccno(int accno) {
		this.accno = accno;
	}
	
	
}
