package com.kapil.beans;

public class CalculateDiscount {
	
	private double amount;
	private double discount;
	
	public CalculateDiscount() {
		amount=0;
		discount=0;
	}

	public double getDiscount() {
		return discount;
	}

	public void setAmount(double amount) {
		this.amount = amount;
		if(amt>15000) {
			disc=amt*15/100;
		}
		else {
			disc=amt*8/100;
		}
	}
	
	
}
