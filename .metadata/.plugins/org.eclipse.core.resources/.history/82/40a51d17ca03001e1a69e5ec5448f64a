package com.kapil.beans;

public class AccountFinder {

	private int accno;
	private double bal;
	private String accnm, atype;
	
	public void AccountFinder() {
		accno=0;
		accnm="Not Found";
		atype="Not Found";
		bal=0.0;
	}

	public double getBal() {
		return bal;
	}

	public String getAccnm() {
		return accnm;
	}

	public String getAtype() {
		return atype;
	}

	public void setAccno(int accno) {
		this.accno = accno;
		retriveData();
	}
	
	
	
}
